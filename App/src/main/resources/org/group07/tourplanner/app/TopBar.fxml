<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<MenuBar xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="org.group07.tourplanner.app.controller.TopBarController">
    <Menu text="%Menu_File">
        <MenuItem text="%Menu_File_Import"/>
        <MenuItem text="%Menu_File_Export"/>
        <MenuItem text="%Menu_File_Quit" onAction="#onMenuFileQuitClicked"/>
    </Menu>
    <Menu text="%Menu_Edit">
        <MenuItem text="%Menu_Edit_Duplicate"/>
    </Menu>
    <Menu text="%Menu_Options">
        <MenuItem mnemonicParsing="false" text="%Menu_Options_Settings"/>
        <MenuItem text="%PDF_TOURREPORT" onAction="#onMenuTourReportClicked"/>
        <MenuItem text="%PDF_SUMMARIZEDREPORT" onAction="#onMenuSummarizedReportClicked"/>
    </Menu>
    <Menu text="%Menu_Help">
        <MenuItem text="%Menu_Help_About" onAction="#onMenuHelpAboutClicked"/>
    </Menu>
</MenuBar>
